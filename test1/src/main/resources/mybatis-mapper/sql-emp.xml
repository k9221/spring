<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.test1.mapper.EmpMapper">
	<select id="selectEmpList" parameterType="hashmap" resultType="com.example.test1.model.Emp">
		SELECT
			EMPNO,
			ENAME,
			JOB,
			MGR,
			HIREDATE,
			SAL,
			COMM,
			DEPTNO
		FROM EMP
	</select>
	
	<select id="empList" parameterType="hashmap" resultType="com.example.test1.model.Emp"> <!--1@: id= 다른 이름으로 설정해준다. 2@: 리턴타입이 Studnet-->
		SELECT * 
		FROM STUDENT 
			
	</select>
	
	
</mapper>